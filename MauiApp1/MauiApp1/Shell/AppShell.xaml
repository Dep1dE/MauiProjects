<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell" 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Flyout"
    Title="MauiApp1">

    

    <FlyoutItem Title="Calculator">
        <ShellContent ContentTemplate="{DataTemplate local:MainPage}"  Title="Calculator">
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="PersonalCalculator">
        <ShellContent ContentTemplate="{DataTemplate local:PersonalCalculator}"  Title="PersonalCalculator">
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="ProgresDemo">
        <ShellContent ContentTemplate="{DataTemplate local:ProgresDemo}"  Title="ProgresDemo">
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="SQLiteDemo">
        <ShellContent ContentTemplate="{DataTemplate local:SQLiteDemo}"  Title="SQLiteDemo">
        </ShellContent>
    </FlyoutItem>

    <FlyoutItem Title="RateDemo">
        <ShellContent ContentTemplate="{DataTemplate local:RateDemo}"  Title="RateDemo">
        </ShellContent>
    </FlyoutItem>

</Shell>
